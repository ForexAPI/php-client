{
    "name": "forexapi/client",
    "homepage": "https://forexapi.eu",
    "description": "Library for consuming the ForexAPI. Ideal for those in the financial or e-commerce sectors, this client simplifies the process of incorporating real-time forex data into various PHP-based projects.",
    "keywords": [
        "forex",
        "api",
        "client",
        "exchange",
        "rates",
        "currency",
        "converter",
        "exchange",
        "currency",
        "finance"
    ],
    "type": "library",
    "license": "MIT",
    "support": {
        "email": "support@forexapi.eu",
        "docs": "https://forexapi.eu/en/docs",
        "issues": "https://github.com/ForexAPI/php-client/issues"
    },
    "funding": [
        {
            "type": "other",
            "url": "https://forexapi.eu/pl/pricing"
        }
    ],
    "autoload": {
        "psr-4": {
            "ForexAPI\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ForexAPI\\Tests\\Client\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "ForexAPI",
            "email": "hello@forexapi.eu"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "psr/http-client-implementation": "^1.0",
        "psr/http-message": "^2.0",
        "php-http/discovery": "^1.19",
        "psr/http-factory": "^1.0",
        "psr/http-factory-implementation": "^1.0"
    },
    "require-dev": {
        "php-http/mock-client": "^1.6",
        "php-http/message": "^1.16",
        "nyholm/psr7": "^1.8",
        "php-http/vcr-plugin": "^1.2",
        "phpstan/phpstan": "^1.10",
        "php-http/curl-client": "^2.3",
        "friendsofphp/php-cs-fixer": "^3.4",
        "phpunit/phpunit": "^8.5"
    },
    "archive": {
        "exclude": [
            "/*",
            "/.phpunit.cache",
            "/.phpunit.result.cache",
            "/.php-cs-fixer.cache",
            "/.php-cs-fixer.dist.php",
            "/.github",
            "/.idea",
            "!/src",
            "!/composer.json",
            "!/composer.lock",
            "!/README.md",
            "!/LICENSE"
        ]
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": false
        }
    }
}
